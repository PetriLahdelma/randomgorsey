{
  "homepage": "https://randomgorsey.com",
  "name": "randomgorsey",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@heroicons/react": "^2.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.5",
    "framer-motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.1",
    "react-social-icons": "^6.24.0",
    "typescript": "^5.9.3",
    "webpack": "^5.99.9",
    "zod": "^4.1.12"
  },
  "scripts": {
    "predeploy": "npm run build && node scripts/copy-cname.js",
    "build": "react-scripts build",
    "postbuild": "react-snap && node scripts/copy-404.js",
    "deploy": "gh-pages -d build --dotfiles",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "stylelint 'src/**/*.css' --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-meta": "node scripts/generate-meta.js",
    "docs:update": "node scripts/update-docs.js update",
    "docs:report": "node scripts/update-docs.js report",
    "build:complete": "npm run build && npm run docs:update",
    "ts:check": "node mcp/direct-checker.js",
    "ts:check:types": "node mcp/direct-checker.js check",
    "ts:check:lint": "node mcp/direct-checker.js lint",
    "ts:check:lint:fix": "node mcp/direct-checker.js lint --fix",
    "ts:check:analyze": "node mcp/direct-checker.js analyze",
    "ts:check:structure": "node mcp/direct-checker.js structure",
    "ts:check:all": "node mcp/direct-checker.js all",
    "mcp:server": "node mcp/typescript-server.js",
    "mcp:checker": "node mcp/typescript-checker.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^react-router-dom$": "<rootDir>/node_modules/react-router-dom/dist/index.js",
      "^react-router/dom$": "<rootDir>/node_modules/react-router/dist/development/dom-export.js"
    }
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox"
    ],
    "include": [
      "/",
      "/about",
      "/contact",
      "/gallery",
      "/discography"
    ],
    "exclude": [
      "/listen"
    ],
    "skipThirdPartyRequests": true
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@modelcontextprotocol/sdk": "^1.21.1",
    "@storybook/addon-a11y": "^10.0.6",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/preset-create-react-app": "^10.0.6",
    "@storybook/react-webpack5": "^10.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^10.0.6",
    "gh-pages": "^6.3.0",
    "prop-types": "^15.8.1",
    "react-scripts": "^5.0.1",
    "react-snap": "^1.23.0",
    "semver": "^7.5.4",
    "storybook": "^10.0.6",
    "stylelint": "^16.20.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^39.0.1"
  },
  "overrides": {
    "minimist": "^1.2.8"
  }
}
