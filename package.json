{
  "homepage": "https://randomgorsey.com",
  "name": "randomgorsey",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@heroicons/react": "^2.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.5",
    "framer-motion": "^12.18.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.1",
    "react-social-icons": "^6.24.0",
    "typescript": "^4.9.5",
    "webpack": "^5.99.9",
    "zod": "^3.25.64"
  },
  "scripts": {
    "predeploy": "npm run build && node scripts/copy-cname.js",
    "build": "react-scripts build",
    "postbuild": "react-snap && node scripts/copy-404.js",
    "deploy": "gh-pages -d build --dotfiles",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "stylelint 'src/**/*.css' --fix && stylelint 'src/**/*.{ts,tsx}' --custom-syntax postcss-js --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-meta": "node scripts/generate-meta.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^react-router-dom$": "<rootDir>/node_modules/react-router-dom/dist/index.js",
      "^react-router/dom$": "<rootDir>/node_modules/react-router/dist/development/dom-export.js"
    }
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox"
    ],
    "include": [
      "/",
      "/about",
      "/contact",
      "/gallery",
      "/discography"
    ],
    "exclude": [
      "/listen"
    ],
    "skipThirdPartyRequests": true
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.0.12",
    "@storybook/addon-docs": "^9.0.12",
    "@storybook/preset-create-react-app": "^9.0.12",
    "@storybook/react-webpack5": "^9.0.12",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^9.0.12",
    "gh-pages": "^6.3.0",
    "prop-types": "^15.8.1",
    "react-scripts": "^5.0.1",
    "react-snap": "^1.23.0",
    "semver": "^7.5.4",
    "storybook": "^9.0.12",
    "stylelint": "^16.20.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "postcss-js": "^4.0.1"
  },
  "overrides": {
    "minimist": "^1.2.8"
  }
}
